{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"components/*": ["app/components/*"],
			"constants/*": ["app/constants/*"],
			"ui/*": ["app/components/ui/*"],
			"screens/*": ["app/components/screens/*"],
			"data/*": ["app/data/*"],
			"interfaces/*": ["app/interfaces/*"],
			"hooks/*": ["app/hooks/*"],
			"services/*": ["app/services/*"],
			"utils/*": ["app/utils/*"],
			"assets/*": ["app/assets/*"],
			"configs/*": ["app/configs/*"],
			"store/*": ["app/store/*"],
			"app/*": ["app/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
