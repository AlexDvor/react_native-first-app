{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./app/",
		"paths": {
			"@/components/*": ["components/*"],
			"@/constants/*": ["constants/*"],
			"@/ui/*": ["components/ui/*"],
			"@/screens/*": ["components/screens/*"],
			"@/interfaces/*": ["interfaces/*"],
			"@/hooks/*": ["hooks/*"],
			"@/services/*": ["services/*"],
			"@/utils/*": ["utils/*"],
			"@/assets/*": ["assets/*"],
			"@/configs/*": ["configs/*"],
			"@/store/*": ["store/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
